#
# per job/run config
#

# Project/namespace. If node defined, default is crucible-rickshaw
export OCP_PROJECT=crucible-regulus

# Jobs, a list. This list conforms to GNU make i.e tab in each line, and no quote ""
export JOBS= \
	./1_GROUP/NO-PAO/4IP/INTER-NODE/TCP/16-POD \
	./1_GROUP/NO-PAO/4IP/INTER-NODE/UDP/16-POD \
	./1_GROUP/PAO/4IP/INTER-NODE/TCP/16-POD \
	./1_GROUP/PAO/4IP/INTER-NODE/UDP/16-POD \
	\
	./1_GROUP/NO-PAO/4IP/INTRA-NODE/TCP/16-POD \
	./1_GROUP/NO-PAO/4IP/INTRA-NODE/UDP/16-POD \
	./1_GROUP/PAO/4IP/INTRA-NODE/TCP/16-POD \
	./1_GROUP/PAO/4IP/INTRA-NODE/UDP/16-POD \
	\
	./3_GROUP/PAO/4IP/BE \
	./3_GROUP/PAO/4IP/QOS

# Debug. DRY run. Default is false. Set to true to DRY
export DRY_RUN=false

# Artifact directory prefix. Default is "".  For example: run-AUTO-18feb-UUDID....
export TAG=AUTO

# Number of samples per test. Default is 3
export NUM_SAMPLES=3

# The duration in seconds to run each test iteration. Default is 120
export DURATION=120
